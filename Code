#include <iostream>
#include<string>
using namespace std;

int main() {

    int arr[3][2][3];// [Main Array num][Sub Array in each Main][element storage ]


    int i, j, k, l;


    for (j = 0;j < 3;j++) {
        for (k = 0;k < 3;k++) {
            for (l = 0;l < 3;l++) {
                cout << "Enter number  " << endl;
                cin >> arr[j][k][l];
               
            }
            cout << "Next column" << endl;
        }
        cout << "Net Row" << endl;

    }

    // Orginal :
    int a, b, c;
    for (a = 0;a < 3;a++) {
        for (b = 0;b < 3;b++) {
            cout << "[";
            for (c = 0;c < 3;c++) {
                cout << arr[a][b][c] << " ";

            }
            cout << "]" << endl;
        }
        cout << endl;
    }
    int g;

    cout << " Hello ";
    cin >> g;







    return 0;
}
